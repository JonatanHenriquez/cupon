{% extends 'base.html.twig' %}

{% block title %} Ultimas Ofertas que has comprado{% endblock %}

{% block body %}
    <div class = "container row">
        <div class = "col s12 m12 l12">
            <h1>{{ block('title') }}</h1>

            {% for compra in compras %}
                {% include "AppBundle:default/include:minioferta.html.twig" with {
                    'oferta': compra.oferta,
                    'comprada': true,
                    'fechaCompra': compra.fecha
                } %}
                <div class="divider"></div>
            {% endfor %}
        </div>

        <!--
        <div class="col s12 m12 l2">

        </div>-->

    </div>

{% endblock %}

{% block javascript %}

{% endblock %}

